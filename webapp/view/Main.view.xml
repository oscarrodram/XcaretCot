                            </uiTable:Column>
                            <uiTable:Column id="colNombre" width="10rem" sortProperty="MATERIAL_NAME" filterProperty="MATERIAL_NAME" visible="{columnVisibility>/colNombre}">
                                <Label id="labelColNombre" text="{i18n>colNombre}"/>
                                <uiTable:template>
