                                </uiTable:template>
                            </uiTable:Column>
                            <uiTable:Column id="colEstatusRequerimiento" width="11.25rem" sortProperty="STATUS_DESC_ES" filterProperty="STATUS_DESC_ES" visible="{columnVisibility>/colEstatusRequerimiento}">
                                <Label id="labelColEstatusRequerimiento" text="{i18n>colEstatusRequerimiento}"/>
