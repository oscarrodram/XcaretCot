                            <uiTable:Column id="colMarca" width="7.5rem" sortProperty="MAT_BRAND" filterProperty="MAT_BRAND" visible="{columnVisibility>/colMarca}">
                                <Label id="labelColMarca" text="{i18n>colMarca}"/>
                                <uiTable:template>
                                    <Text id="textColMarca" text="{MAT_BRAND}" />
                                </uiTable:template>
                            </uiTable:Column>
                            <uiTable:Column id="colModelo" width="7.5rem" sortProperty="MAT_MODEL" filterProperty="MAT_MODEL" visible="{columnVisibility>/colModelo}">
                                <Label id="labelColModelo" text="{i18n>colModelo}"/>
