                                    suggest="onSuggestFilters"
                                    tokens="{path: 'multiSelect>/inputCotizacion', templateShareable: false}"
                                    tokenUpdate="onTokenUpdate">
                                    <tokens>
                                        <Token text="{multiSelect>}" key="{multiSelect>}" />
                                    </tokens>
                                </MultiInput>
                            </VBox>
                            <VBox id="vboxFilterPedidoXdifica" width="220px" class="sapUiSmallMarginBegin" visible="{visibility>/vboxFilterPedidoXdifica}">
                                <Label id="labelPedidoXdifica" text="{i18n>filterPedidoXdifica}"/>
                                <MultiInput id="inputPedidoXdifica"
                                    showValueHelp="true"
                                    valueHelpRequest="onValueHelpRequest"
                                    showSuggestion="true"
                                    suggest="onSuggestFilters"
                                    tokens="{path: 'multiSelect>/inputPedidoXdifica', templateShareable: false}"
                                    tokenUpdate="onTokenUpdate">
                                    <tokens>
                                        <Token text="{multiSelect>}" key="{multiSelect>}" />
                                    </tokens>
                                </MultiInput>
                            </VBox>
                            <VBox id="vboxFilterProveedor" width="220px" class="sapUiSmallMarginBegin" visible="{visibility>/vboxFilterProveedor}">
                                <Label id="labelProveedor" text="{i18n>filterProveedor}"/>
                                <MultiInput id="inputProveedor"
                                    showValueHelp="true"
                                    valueHelpRequest="onValueHelpRequest"
                                    showSuggestion="true"
                                    suggest="onSuggestFilters"
                                    tokens="{path: 'multiSelect>/inputProveedor', templateShareable: false}"
                                    tokenUpdate="onTokenUpdate">
                                    <tokens>
                                        <Token text="{multiSelect>}" key="{multiSelect>}" />
                                    </tokens>
                                </MultiInput>
                            </VBox>
                            <VBox id="vboxFilterMaterial" width="220px" class="sapUiSmallMarginBegin" visible="{visibility>/vboxFilterMaterial}">
                                <Label id="labelMaterial" text="{i18n>filterMaterial}"/>
                                <MultiInput id="inputMaterial"
                                    showValueHelp="true"
                                    valueHelpRequest="onValueHelpRequest"
                                    showSuggestion="true"
                                    suggest="onSuggestFilters"
                                    tokens="{path: 'multiSelect>/inputMaterial', templateShareable: false}"
                                    tokenUpdate="onTokenUpdate">
                                    <tokens>
                                        <Token text="{multiSelect>}" key="{multiSelect>}" />
                                    </tokens>
                                </MultiInput>
                            </VBox>
                            <VBox id="vboxFilterCategoria" width="220px" class="sapUiSmallMarginBegin" visible="{visibility>/vboxFilterCategoria}">
                                <Label id="labelCategoria" text="{i18n>filterCategoria}"/>
                                <MultiInput id="inputCategoria"
                                    showValueHelp="true"
                                    valueHelpRequest="onValueHelpRequest"
                                    showSuggestion="true"
                                    suggest="onSuggestFilters"
                                    tokens="{path: 'multiSelect>/inputCategoria', templateShareable: false}"
                                    tokenUpdate="onTokenUpdate">
                                    <tokens>
                                        <Token text="{multiSelect>}" key="{multiSelect>}" />
                                    </tokens>
                                </MultiInput>
                            </VBox>
                            <VBox id="vboxFilterFamilia" width="220px" class="sapUiSmallMarginBegin" visible="{visibility>/vboxFilterFamilia}">
                                <Label id="labelFamilia" text="{i18n>filterFamilia}"/>
                                <MultiInput id="inputFamilia"
                                    showValueHelp="true"
                                    valueHelpRequest="onValueHelpRequest"
                                    showSuggestion="true"
                                    suggest="onSuggestFilters"
                                    tokens="{path: 'multiSelect>/inputFamilia', templateShareable: false}"
                                    tokenUpdate="onTokenUpdate">
                                    <tokens>
                                        <Token text="{multiSelect>}" key="{multiSelect>}" />
                                    </tokens>
                                </MultiInput>
                            </VBox>
                            <VBox id="vboxFilterMarca" width="220px" class="sapUiSmallMarginBegin" visible="{visibility>/vboxFilterMarca}">
                                <Label id="labelMarca" text="{i18n>filterMarca}"/>
                                <MultiInput id="inputMarca"
                                    showValueHelp="true"
                                    valueHelpRequest="onValueHelpRequest"
                                    showSuggestion="true"
                                    suggest="onSuggestFilters"
                                    tokens="{path: 'multiSelect>/inputMarca', templateShareable: false}"
                                    tokenUpdate="onTokenUpdate">
                                    <tokens>
                                        <Token text="{multiSelect>}" key="{multiSelect>}" />
                                    </tokens>
                                </MultiInput>
                            </VBox>
                            <VBox id="vboxFilterModelo" width="220px" class="sapUiSmallMarginBegin" visible="{visibility>/vboxFilterModelo}">
                                <Label id="labelModelo" text="{i18n>filterModelo}"/>
                                <MultiInput id="inputModelo"
                                    showValueHelp="true"
                                    valueHelpRequest="onValueHelpRequest"
                                    showSuggestion="true"
                                    suggest="onSuggestFilters"
                                    tokens="{path: 'multiSelect>/inputModelo', templateShareable: false}"
                                    tokenUpdate="onTokenUpdate">
                                    <tokens>
                                        <Token text="{multiSelect>}" key="{multiSelect>}" />
                                    </tokens>
                                </MultiInput>
                            </VBox>
                            <VBox id="vboxFilterDivision" width="220px" class="sapUiSmallMarginBegin" visible="{visibility>/vboxFilterDivision}">
                                <Label id="labelDivision" text="{i18n>filterDivision}"/>
                                <MultiInput id="inputDivision"
                                    showValueHelp="true"
                                    valueHelpRequest="onValueHelpRequest"
                                    showSuggestion="true"
                                    suggest="onSuggestFilters"
                                    tokens="{path: 'multiSelect>/inputDivision', templateShareable: false}"
                                    tokenUpdate="onTokenUpdate">
                                    <tokens>
                                        <Token text="{multiSelect>}" key="{multiSelect>}" />
                                    </tokens>
                                </MultiInput>
                            </VBox>
                            <VBox id="vboxFilterArea" width="220px" class="sapUiSmallMarginBegin" visible="{visibility>/vboxFilterArea}">
                                <Label id="labelArea" text="{i18n>filterArea}"/>
                                <MultiInput id="inputArea"
                                    showValueHelp="true"
                                    valueHelpRequest="onValueHelpRequest"
                                    showSuggestion="true"
                                    suggest="onSuggestFilters"
                                    tokens="{path: 'multiSelect>/inputArea', templateShareable: false}"
                                    tokenUpdate="onTokenUpdate">
                                    <tokens>
                                        <Token text="{multiSelect>}" key="{multiSelect>}" />
                                    </tokens>
                                </MultiInput>
                            </VBox>
                            <VBox id="vboxFilterUbicacion" width="220px" class="sapUiSmallMarginBegin" visible="{visibility>/vboxFilterUbicacion}">
                                <Label id="labelUbicacion" text="{i18n>filterUbicacion}"/>
                                <MultiInput id="inputUbicacion"
                                    showValueHelp="true"
                                    valueHelpRequest="onValueHelpRequest"
                                    showSuggestion="true"
                                    suggest="onSuggestFilters"
