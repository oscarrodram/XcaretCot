                            </uiTable:template>
                        </uiTable:Column>
                        <uiTable:Column id="colNombre" width="10rem" sortProperty="MATERIAL_NAME" filterProperty="MATERIAL_NAME" visible="{columnVisibility>/colNombre}">
                            <Label id="labelColNombre" text="{i18n>colNombre}"/>
                            <uiTable:template>
                                <Text id="textColNombre" text="{MATERIAL_NAME}" />
                            </uiTable:template>
                        </uiTable:Column>
                        <uiTable:Column id="colDescripcion" width="12.5rem" sortProperty="MATERIAL_DESC" filterProperty="MATERIAL_DESC" visible="{columnVisibility>/colDescripcion}">
                            <Label id="labelColDescripcion" text="{i18n>colDescripcion}"/>
                            <uiTable:template>
