                                    tokenUpdate="onTokenUpdate">
                                    <tokens>
                                        <Token text="{multiSelect>}" key="{multiSelect>}" />
                                    </tokens>
                                </MultiInput>
                            </VBox>
                            <VBox id="vboxFilterSubUbicacion" width="220px" class="sapUiSmallMarginBegin" visible="{visibility>/vboxFilterSubUbicacion}">
                                <Label id="labelSubUbicacion" text="{i18n>filterSubUbicacion}"/>
                                <MultiInput id="inputSubUbicacion"
                                    showValueHelp="true"
                                    valueHelpRequest="onValueHelpRequest"
                                    showSuggestion="true"
                                    suggest="onSuggestFilters"
                                    tokens="{path: 'multiSelect>/inputSubUbicacion', templateShareable: false}"
                                    tokenUpdate="onTokenUpdate">
                                    <tokens>
                                        <Token text="{multiSelect>}" key="{multiSelect>}" />
                                    </tokens>
                                </MultiInput>
                            </VBox>
                            <VBox id="vboxFilterRequerimiento" width="220px" class="sapUiSmallMarginBegin" visible="{visibility>/vboxFilterRequerimiento}">
                                <Label id="labelRequerimiento" text="{i18n>filterRequerimiento}"/>
                                <MultiInput id="inputRequerimiento"
                                    showValueHelp="true"
                                    valueHelpRequest="onValueHelpRequest"
                                    showSuggestion="true"
                                    suggest="onSuggestFilters"
                                    tokens="{path: 'multiSelect>/inputRequerimiento', templateShareable: false}"
                                    tokenUpdate="onTokenUpdate">
                                    <tokens>
                                        <Token text="{multiSelect>}" key="{multiSelect>}" />
                                    </tokens>
                                </MultiInput>
                            </VBox>
                        </HBox>
                        <Toolbar id="toolbarMainFilters">
                            <ToolbarSpacer id="toolbarSpacerMainFilters"/>
                            <Button id="btnBuscar" text="{i18n>btnBuscar}" type="Emphasized" press="onSearchQuotation"/>
                            <Button id="btnChangeFilters" text="{i18n>btnChangeFilters}" type="Default" press="onChangeFilters"/>
                        </Toolbar>
                    </f:DynamicPageHeader>
            </f:header> 
            <f:content>
                    <uiTable:Table
                        id="quotationTable"
                        title="{= ${i18n>tableTitle} + ' (' + ${/listItems}.length + ')' }"
                        rows="{/listItems}"
                        selectionMode="None"
                        columnHeaderVisible="true"
                        width="auto"
                        visibleRowCountMode="Auto">
                        <uiTable:extension>
                            <Toolbar id="toolBarTableButtons">
                                <ToolbarSpacer id="toolbarSpacerTable"/>
                                <!--<Text id="textTotal" text="{= ${i18n>footerTotalLabel} + ': $' + ${totals>/grandTotal} }"/>-->
                                <Text
                                id="textTotal"
                                text="{parts: [
                                        {path: 'i18n>footerTotalLabel'},
                                        {path: 'totals>/grandTotal', formatter: '.formatter.formatNumberWithCommas'}
                                    ],
                                    formatter: '.formatter.formatTotalText'
                                }"/>
                                <Button icon="sap-icon://excel-attachment"
                                        tooltip="Table Settings"
                                        type="Transparent"
                                        press="onDownloadExcel"
                                        id="buttonDownloadExcel"/>
                                <Button icon="sap-icon://action-settings"
                                        tooltip="Table Settings"
                                        type="Transparent"
                                        press="onSettingsPress"
                                        id="buttonAdaptTableFilters"/>
                            </Toolbar>
                        </uiTable:extension>
                        <uiTable:columns>
                            <uiTable:Column id="colIndBorrado" width="8.125rem" visible="{columnVisibility>/colIndBorrado}">
                                <Label id="labelColIndBorrado" text="{i18n>colIndBorrado}"/>
                                <uiTable:template>
                                    <Switch id="switchColIndBorrado" type="AcceptReject" state="{path: 'LOEKZ', formatter: '.formatter.xFlagToBool'}" enabled="false"/>
                                </uiTable:template>
                            </uiTable:Column>
                            <uiTable:Column id="colNombreProyecto" width="11.25rem" sortProperty="PROJECT_NAME" filterProperty="PROJECT_NAME" visible="{columnVisibility>/colNombreProyecto}">
                                <Label id="labelColNombreProyecto" text="{i18n>colNombreProyecto}"/>
                                <uiTable:template>
                                    <Text id="textColNombreProyecto" text="{PSPNR} / {PROJECT_NAME}" />
                                </uiTable:template>
                            </uiTable:Column>
                            <uiTable:Column id="colFechaCotizacion" width="9.375rem" sortProperty="QUOTATION_DATE" filterProperty="QUOTATION_DATE" visible="{columnVisibility>/colFechaCotizacion}">
                                <Label id="labelColFechaCotizacion" text="{i18n>colFechaCotizacion}"/>
                                <uiTable:template>
                                    <Text id="textColFechaCotizacion" text="{QUOTATION_DATE}" />
                                </uiTable:template>
                            </uiTable:Column>
                            <uiTable:Column id="colNumeroCotizacion" width="10rem" sortProperty="EBELN" filterProperty="EBELN" visible="{columnVisibility>/colNumeroCotizacion}">
                                <Label id="labelColNumeroCotizacion" text="{i18n>colNumeroCotizacion}"/>
                                <uiTable:template>
                                    <Text id="textColNumeroCotizacion" text="{EBELN}" />
                                </uiTable:template>
                            </uiTable:Column>
                            <uiTable:Column id="colPosCotizacion" width="7.5rem" sortProperty="EBEL" filterProperty="EBEL" visible="{columnVisibility>/colPosCotizacion}">
                                <Label id="labelColPosCotizacion" text="{i18n>colPosCotizacion}"/>
                                <uiTable:template>
                                    <Text id="textColPosCotizacion" text="{EBEL}" />
                                </uiTable:template>
                            </uiTable:Column>
                            <uiTable:Column id="colContrato" width="8.75rem" sortProperty="CONTRACT_NAME" filterProperty="CONTRACT_NAME" visible="{columnVisibility>/colContrato}">
                                <Label id="labelColContrato" text="{i18n>colContrato}"/>
                                <uiTable:template>
                                    <Text id="textColContrato" text="{CONTRACT_NAME}" />
                                </uiTable:template>
                            </uiTable:Column>
                            <uiTable:Column id="colPedidoXdifica" width="8.75rem" sortProperty="QUOTATION_PED_XDIFICA" filterProperty="QUOTATION_PED_XDIFICA" visible="{columnVisibility>/colPedidoXdifica}">
                                <Label id="labelColPedidoXdifica" text="{i18n>colPedidoXdifica}"/>
                                <uiTable:template>
                                    <Text id="textColPedidoXdifica" text="{QUOTATION_PED_XDIFICA}" />
                                </uiTable:template>
                            </uiTable:Column>
                            <uiTable:Column id="colProveedor" width="9.375rem" sortProperty="SUPPLIER_NAME" filterProperty="SUPPLIER_NAME" visible="{columnVisibility>/colProveedor}">
                                <Label id="labelColProveedor" text="{i18n>colProveedor}"/>
                                <uiTable:template>
                                    <Text id="textColProveedor" text="{SUPPLIER_NAME}" />
                                </uiTable:template>
                            </uiTable:Column>
                            <uiTable:Column id="colCargo" width="6.25rem" sortProperty="QUOTATION_CHARGE" filterProperty="QUOTATION_CHARGE" visible="{columnVisibility>/colCargo}">
                                <Label id="labelColCargo" text="{i18n>colCargo}"/>
