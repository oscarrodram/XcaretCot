<mvc:View controllerName="comxcaretrepcot.comxcaretrepcot.controller.Main"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:f="sap.f"
    xmlns:uiTable="sap.ui.table">
    <Page id="page">
        <f:DynamicPage id="dynamicPageMain" showFooter="false" fitContent="false"> 
            <f:header>
                    <f:DynamicPageHeader id="dynamicPageHeaderMain">
                        <HBox id="hboxFilterMain" wrap="Wrap" alignItems="Start" justifyContent="Start">
                            <VBox id="vboxFilterFechaCotizacion" width="220px" class="sapUiSmallMarginBegin" visible="{visibility>/vboxFilterFechaCotizacion}">
                                <Label id="labelFechaCotizacion" text="{i18n>filterFechaCotizacion}"/>
                                <!--<DatePicker id="dpFechaCotizacion" width="100%" />-->
                                <DateRangeSelection id="dpFechaCotizacion" width="100%" delimiter=" - " />
                            </VBox>
                            <VBox id="vboxFilterProyecto" width="220px" class="sapUiSmallMarginBegin" visible="{visibility>/vboxFilterProyecto}">
                                <Label id="labelProyecto" text="{i18n>filterProyecto}"/>
                                <MultiInput id="inputNombreProyecto"
                                    showValueHelp="true"
                                    valueHelpRequest="onValueHelpRequest"
                                    showSuggestion="true"
                                    suggest="onSuggestFilters"
                                    tokens="{path: 'multiSelect>/inputNombreProyecto', templateShareable: false}"
                                    tokenUpdate="onTokenUpdate">
                                    <tokens>
                                        <Token text="{multiSelect>}" key="{multiSelect>}" />
                                    </tokens>
                                </MultiInput>
                            </VBox>
                            <VBox id="vboxFilterCotizacion" width="220px" class="sapUiSmallMarginBegin" visible="{visibility>/vboxFilterCotizacion}">
