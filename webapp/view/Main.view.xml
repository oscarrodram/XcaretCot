                            </uiTable:template>
                        </uiTable:Column>
                        <uiTable:Column id="colDivision" width="8.75rem" sortProperty="REQ_DIVISION_DESC" filterProperty="REQ_DIVISION_DESC" visible="{columnVisibility>/colDivision}">
