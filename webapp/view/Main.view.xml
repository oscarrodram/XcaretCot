                                </uiTable:template>
                            </uiTable:Column>
                            <uiTable:Column id="colPatrimonio" width="7.5rem" visible="{columnVisibility>/colPatrimonio}">
                                <Label id="labelColPatrimonio" text="{i18n>colPatrimonio}"/>
                                <uiTable:template>
                                    <Switch id="switchColPatrimonio" type="AcceptReject" state="{path: 'MAT_PATRIMONIO', formatter: '.formatter.xFlagToBool'}" enabled="false"/>
                                </uiTable:template>
                            </uiTable:Column>
                            <uiTable:Column id="colEspeciales" width="7.5rem" visible="{columnVisibility>/colEspeciales}">
                                <Label id="labelColEspeciales" text="{i18n>colEspeciales}"/>
                                <uiTable:template>
                                    <Switch id="switchColEspeciales" type="AcceptReject" state="{path: 'MAT_SPECIAL', formatter: '.formatter.xFlagToBool'}" enabled="false"/>
