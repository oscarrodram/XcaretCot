                            </uiTable:Column>
                            <uiTable:Column id="colTipoCambio" width="8.125rem" sortProperty="QUOTATION_EXCHANGE_RATE" filterProperty="QUOTATION_EXCHANGE_RATE" visible="{columnVisibility>/colTipoCambio}">
                                <Label id="labelColTipoCambio" text="{i18n>colTipoCambio}"/>
                                <uiTable:template>
                                    <Text id="textColTipoCambio" text="{QUOTATION_EXCHANGE_RATE}" />
                                </uiTable:template>
                            </uiTable:Column>
                            <uiTable:Column id="colMontoMN" width="7.5rem" sortProperty="TOTAL_AMOUNT_ITEM" filterProperty="TOTAL_AMOUNT_ITEM" visible="{columnVisibility>/colMontoMN}">
                                <Label id="labelColMontoMN" text="{i18n>colMontoMN}"/>
                                <uiTable:template>
                                    <Text id="textColMontoMN" text="{path: 'TOTAL_AMOUNT_ITEM', formatter: '.formatter.formatNumberWithCommas'}" />
                                </uiTable:template>
                            </uiTable:Column>
                            <uiTable:Column id="colCantidadCotizada" width="10rem" sortProperty="KTMNG" filterProperty="KTMNG" visible="{columnVisibility>/colCantidadCotizada}">
                                <Label id="labelColCantidadCotizada" text="{i18n>colCantidadCotizada}"/>
                                <uiTable:template>
                                    <Text id="textColCantidadCotizada" text="{KTMNG}" />
                                </uiTable:template>
                            </uiTable:Column>
                            <uiTable:Column id="colCantidadFaltante" width="10rem" sortProperty="MENGE_C" filterProperty="MENGE_C" visible="{columnVisibility>/colCantidadFaltante}">
                                <Label id="labelColCantidadFaltante" text="{i18n>colCantidadFaltante}"/>
                                <uiTable:template>
                                    <Text id="textColCantidadFaltante" text="{MENGE_C}" />
                                </uiTable:template>
                            </uiTable:Column>
                            <uiTable:Column id="colCategoria" width="8.75rem" sortProperty="CATEGORY_DESC" filterProperty="CATEGORY_DESC" visible="{columnVisibility>/colCategoria}">
                                <Label id="labelColCategoria" text="{i18n>colCategoria}"/>
                                <uiTable:template>
                                    <Text id="textColCategoria" text="{CATEGORY_DESC}" />
                                </uiTable:template>
                            </uiTable:Column>
                            <uiTable:Column id="colFamilia" width="8.75rem" sortProperty="FAMILY_DESC" filterProperty="FAMILY_DESC" visible="{columnVisibility>/colFamilia}">
                                <Label id="labelColFamilia" text="{i18n>colFamilia}"/>
                                <uiTable:template>
