                        <uiTable:Column id="colFFEGMFFEDI" width="10rem" sortProperty="REQ_FFE" filterProperty="REQ_FFE" visible="{columnVisibility>/colFFEGMFFEDI}">
                            <Label id="labelColFFEGMFFEDI" text="{i18n>colFFEGMFFEDI}"/>
                            <uiTable:template>
                                <Text id="textColFFEGMFFEDI" text="{REQ_FFE}" />
                            </uiTable:template>
                        </uiTable:Column>
                        <uiTable:Column id="colActivoFijo" width="7.5rem" visible="{columnVisibility>/colActivoFijo}">
                            <Label id="labelColActivoFijo" text="{i18n>colActivoFijo}"/>
                            <uiTable:template>
                                <Switch id="switchColActivoFijo" type="AcceptReject" state="{path: 'MAT_FIXED_ASSET', formatter: '.formatter.xFlagToBool'}" enabled="false"/>
                            </uiTable:template>
                        </uiTable:Column>
                        <uiTable:Column id="colEstandar" width="7.5rem" visible="{columnVisibility>/colEstandar}">
                            <Label id="labelColEstandar" text="{i18n>colEstandar}"/>
                            <uiTable:template>
                                <Switch id="switchColEstandar" type="AcceptReject" state="{path: 'MAT_STANDARD', formatter: '.formatter.xFlagToBool'}" enabled="false"/>
                            </uiTable:template>
                        </uiTable:Column>
                        <uiTable:Column id="colPatrimonio" width="7.5rem" visible="{columnVisibility>/colPatrimonio}">
