                            </uiTable:template>
                        </uiTable:Column>
                        <uiTable:Column id="colEspeciales" width="7.5rem" visible="{columnVisibility>/colEspeciales}">
                            <Label id="labelColEspeciales" text="{i18n>colEspeciales}"/>
                            <uiTable:template>
