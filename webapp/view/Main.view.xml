                            </uiTable:template>
                        </uiTable:Column>
                        <uiTable:Column id="colNoMaterial" width="7.5rem" sortProperty="MATNR" filterProperty="MATNR" visible="{columnVisibility>/colNoMaterial}">
                            <Label id="labelColNoMaterial" text="{i18n>colNoMaterial}"/>
                            <uiTable:template>
